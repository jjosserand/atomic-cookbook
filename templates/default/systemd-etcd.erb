#
# Configuration file generated by Chef for <%= node['fqdn'] %>
# Any changes will potentially be overwritten.
#
[Unit]
Description=Etcd Server
After=network.target

[Service]
Type=simple
# etc logs to the journal directly, suppress double logging
StandardOutput=null
WorkingDirectory=/var/lib/etcd
User=etcd
ExecStart=/usr/bin/etcd --listen-client-urls=http://0.0.0.0:2379,http://0.0.0.0:4001

[Install]
WantedBy=multi-user.target
